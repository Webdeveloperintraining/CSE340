<section>
<% if (title /*&& locals.accountsData.account_type !== 'Admin'*/) { %>
    <h1><%= title %></h1>

  <% } else {
      res.redirect('/')
    } %>
    <%- messages() %>
    <% if (errors) { %>

    <ul class="notice">
    <% errors.array().forEach(error => { %>
    <li><%= error.msg %></li>
    <%  }) %>
    </ul>
    <% } %>

  <p>Choose an account to modify their properties:</p>
  <%- accountsSelect  %>
  <table id="AccountDisplay"></table>
  <noscript>JavaScript must be enabled to use this page.</noscript>

</section>